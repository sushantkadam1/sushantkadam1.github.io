		function toggleNav() {
			var menuDisplayState = $("#mobile_nav").css('display');
			if (menuDisplayState === 'block') {
				$("#mobile_nav").fadeOut(300);
				$("#toggle_state").html('+');
			} else {
				$("#mobile_nav").fadeIn(300);
				$("#toggle_state").html('-');
			}
		}
	</script>
